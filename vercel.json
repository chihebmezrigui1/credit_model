{
    "version": 2,
    "builds": [
      {
        "src": "application.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/style.css",
        "dest": "/style.css"
      },
      {
        "src": "/",
        "dest": "/index.html"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  