{
    "version": 2,
    "builds": [
      {
        "src": "application.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/static/(.*)",
        "dest": "/static/$1"
      },
      {
        "src": "/",
        "dest": "templates/index.html"
      }
    ]
  }
  